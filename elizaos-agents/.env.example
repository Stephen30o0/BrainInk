# Environment variables for ElizaOS Brain Ink integration

# Required: OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Other model providers
ANTHROPIC_API_KEY=your_anthropic_key_here
GOOGLE_GENERATIVE_AI_API_KEY=your_google_key_here

# Brain Ink Backend Integration
KANA_API_URL=http://localhost:10000
BRAIN_INK_FRONTEND_URL=http://localhost:5173

# Database (use pglite for simplicity)
DATABASE_URL=

# Logging
LOG_LEVEL=info

# ElizaOS Configuration
ELIZA_DEBUG=false
NODE_ENV=development

# Brain Ink specific settings
BRAININK_INTEGRATION_ENABLED=true
BRAININK_TOURNAMENT_WEBHOOK=
BRAININK_PROGRESS_SYNC=true
