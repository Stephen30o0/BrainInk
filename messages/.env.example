# BrainInk XMTP + AgentKit AI Study Bot Environment Variables
# Copy this file to .env and fill in your actual values

# Gemini AI API Key (required)
GEMINI_API_KEY=your_gemini_api_key_here

# Wallet Configuration for XMTP (required)
WALLET_PRIVATE_KEY=your_wallet_private_key_here
WALLET_ADDRESS=your_wallet_address_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Base Sepolia Contract Addresses (deployed contracts)
XP_TOKEN_ADDRESS=0x...
BADGE_NFT_ADDRESS=0x...
SQUAD_SCORE_ADDRESS=0x...
BOUNTY_VAULT_ADDRESS=0x...

# XMTP Configuration
XMTP_ENV=dev

# Database Configuration (optional for mock data)
DATABASE_URL=your_database_url_here

# CORS Configuration
CORS_ORIGIN=http://localhost:3000
