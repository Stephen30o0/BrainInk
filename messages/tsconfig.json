{  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022"],
    "outDir": "./dist",
    "rootDir": ".",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "typeRoots": ["./node_modules/@types"],
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "allowJs": true
  },"include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./index.ts",
    "./services/**/*",
    "./components/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "../**"
  ]
}
