{
  "version": 2,
  "builds": [
    {
      "src": "quiz/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/quiz/dist/assets/$1" },
    { "src": "/(.*)", "dest": "/quiz/dist/index.html" }
  ]
}
