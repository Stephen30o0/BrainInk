{
  "version": 2,
  "builds": [
    {
      "src": "quiz/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" } 
    },
    {
      "src": "api/kana.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/kana/(.*)",
      "dest": "/api/kana.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
