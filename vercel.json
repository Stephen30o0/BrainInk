{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://your-kana-backend.onrender.com/api/$1" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_KANA_API_BASE_URL": "https://your-kana-backend.onrender.com/api/kana"
  }
}